{
    "name": "pixelspin/silverstripe-googleanalyticsdata",
    "description": "Google analytics data for silverstripe 4",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "google",
        "analytics",
        "stats"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Michel van der Steege",
        "email": "michel@thewebmen.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4.0@dev",
        "silverstripe/vendor-plugin": "^1.0",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "silverstripe-googleanalyticsdata",
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Pixelspin\\GoogleAnalyticsData\\": "src/"
        }
    }
}
